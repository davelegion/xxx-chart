//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//font responsive. Keep all charts date above this for it to effect all chart font sizes /////////////////////////////

window.onload = function() {

  //
  console.log(window.innerWidth)
  if (window.innerWidth >= 600) {
    Chart.defaults.font.size = 20;
    //when screensize falls within the following screen size font will change the the follwoing point size
  } else if (window.innerWidth >= 600 && window.innerWidth < 999) {
    Chart.defaults.font.size = 25;
    //otherwise this will it default to 12 - or whatever result delow
  } else {
    Chart.defaults.font.size = 12;
  }
  drawChart()
}
