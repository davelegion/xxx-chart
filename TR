function drawChart() {

  //the drawchart function wraps chart and control global chart font size

///// Chart sheet 5 RADAR VERSION ///////////////////////////////////////////////

  const labels = [
    ['Individual'],
    ['Player'],
    ['Team'],
    ['Organisation']
  ];
  //labels.shift();
  //console.log(labels);
  //labels.pop();


  // data block
  const data5 = {
    labels,
    datasets: [{
      //label: '# of Votes',
      data: [12, 19, 3, 5, 2, 3],
      backgroundColor: [
        'rgba(20, 62, 53, 1)',
        'rgba(102, 127, 121, 1)',
        'rgba(198, 206, 204, 1)',
        'rgba(201, 201, 201, 1)'
      ],

      borderColor: [
        'rgba(255, 99, 132, 1)',
        'rgba(54, 162, 235, 1)',
        'rgba(255, 206, 86, 1)',
        'rgba(75, 192, 192, 1)',
        'rgba(153, 102, 255, 1)',
        'rgba(255, 159, 64, 1)'
      ],
      borderWidth: 0
    }]
  };

  // config block
  const config5 = {
    type: 'bar',
    data: data5,
    options: {
      plugins: {
      // all styling for tooltip
    	tooltip: {
        titleFont: {
          size: 14
        },
        bodyFont: {
          size: 35
        },
        footerFont: {
          size: 20, // there is no footer by default
        },
        backgroundColor: '#FFF9F7',
        displayColors: false,
        titleColor: '#143E35',
        bodyColor: '#143E35',
        footerColor: '#143E35',
        padding:{
        left:20,top:20,right:20,bottom:20,
        }
      },
        legend: {
          display: false,
          title: {
            // This more specific font property overrides the global property
            font: {
              size: 44,
              color: '#143E35'
            }
          }
        }
      },

      //design scales
      scales: {
        x: {
          grid: {
            circular: false,
            lineWidth: 0,
            color: 'rgba(20, 62, 53, .5)',
          },
          ticks: {
            color: '#143E35',
            font: {
              size: 22,
              weight: 'bold',
              display: true,
            },
            pointLabels: {
              color: '#143E35',
              font: {
                size: 22
              }
            }
          }
        }
      }
    }
  };

  // init render block
  const myChart5 = new Chart(
    document.getElementById('myChart5'),
    config5
  );
}

