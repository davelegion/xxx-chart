///// Chart sheet 6 RADAR VERSION ///////////////////////////////////////////////

// data block
const data15 = {
 labels: [
['Contribute to a meaningful', 'of purpose through the', 'word that we do together'],
['Act in accordance to', 'the team’s shared values'],
['Achieve future success in', 'the areas that matter', 'the most'],
['Proactively identify solutions', 'to problems we encounter'],
['Effectively discuss setbacks', 'help to learn form', 'our experience']],
  datasets: [{
    //label: '# of Votes',
    data: [12, 19, 32, 37, 23 ],
    backgroundColor: 'rgba(20, 62, 53, .7)',
    //radar point styling
    borderColor: 'rgb(2s0, 203, 203, 1)',
    pointBackgroundColor: 'rgba(20, 62, 53, 1)',
    pointBorderColor: 'rgba(20, 62, 53, .5)',
    pointHoverBackgroundColor: 'rgba(20, 62, 53, 1)',
    pointHoverBorderColor: 'rgb(20, 99, 132)',
  }]
};

// config block
const config15 = {
  type: 'radar',
  data: data15,
  options: {
    plugins: {
    // all styling for tooltip
    	tooltip: {
        titleFont: {
          size: 14
        },
        bodyFont: {
          size: 35
        },
        footerFont: {
          size: 20, // there is no footer by default
        },
        backgroundColor: '#FFF9F7',
        displayColors: false,
        titleColor: '#143E35',
        bodyColor: '#143E35',
        footerColor: '#143E35',
        padding:{
        left:20,top:20,right:20,bottom:20,
        }
      },
      legend: {
        display: false,
        title: {
          // This more specific font property overrides the global property
          font: {
            size: 22,
            color: '#143E35'
          }
        }
      }
    },

    elements: {
      line: {
        borderWidth: 0
      },
      point: {
        pointRadius: 12
      }
    },

    //design scales
     scales: {
      r: {
        angleLines: {
          lineWidth: 3,
          color: 'rgba(20, 62, 53, .5)',
        },
        grid: {
          circular: false,
          lineWidth: 3,
          color: 'rgba(20, 62, 53, .5)',
        },
        ticks: {
          color: ' #143E35',
          font: {
            size: 18,
            weight: 'bold',
            display: true,
          }
        },
        pointLabels: {
          color: '#143E35',
          font: {
            size: 16
          }
        }
      }
    }
  }
};

// init render block
const myChart15 = new Chart(
  document.getElementById('myChart15'),
  config15
);


